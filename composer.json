{
    "name": "artmoi/laravel-datadog",
    "type": "library",
    "description": "A better Dogstatsd integration with Laravel.",
    "keywords": [
        "laravel",
        "datadog",
        "monolog"
    ],
    "license": "apache-2.0",
    "authors": [
        {
            "name": "Alexander Trauzzi",
            "email": "atrauzzi@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": ">=6",
        "myclabs/php-enum": "^1.6",
        "datadog/php-datadogstatsd": "^1.0",
        "ramsey/uuid": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "ArtMoi\\LaravelDatadog\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArtMoi\\LaravelDatadog\\LaravelDatadogServiceProvider"
            ]
        }
    }
}
